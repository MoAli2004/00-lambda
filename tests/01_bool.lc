-------------------------------------------------------------------------------
-- DO NOT MODIFY THIS SEGMENT
-------------------------------------------------------------------------------

let TRUE  = \x y -> x
let FALSE = \x y -> y
let ITE   = \b x y -> b x y
let NOT   = \b x y -> b y x
let AND   = \b1 b2 -> ITE b1 b2 FALSE
let OR    = \b1 b2 -> ITE b1 TRUE b2

-------------------------------------------------------------------------------
-- YOU SHOULD ONLY MODIFY THE TEXT BELOW, JUST THE PARTS MARKED AS COMMENTS
-------------------------------------------------------------------------------

eval not_true :
  NOT TRUE
  =d> (\b -> \x y -> b y x) TRUE
  =a> (\x y -> TRUE y x)
  =d> (\x y -> (\p q -> p) y x)
  =a> (\x y -> y)
  =b> FALSE

eval and_true_false :
  AND TRUE FALSE
  =d> (\b1 b2 -> ITE b1 b2 FALSE) TRUE FALSE
  =a> ITE TRUE FALSE FALSE
  =d> (\b x y -> b x y) TRUE FALSE FALSE
  =a> TRUE FALSE FALSE
  =d> (\p q -> p) FALSE FALSE
  =a> FALSE
  =b> FALSE

eval or_false_true :
  OR FALSE TRUE
  =d> (\b1 b2 -> ITE b1 TRUE b2) FALSE TRUE
  =a> ITE FALSE TRUE TRUE
  =d> (\b x y -> b x y) FALSE TRUE TRUE
  =a> FALSE TRUE TRUE
  =d> (\p q -> q) TRUE TRUE
  =a> TRUE
  =b> TRUE

                 
 
