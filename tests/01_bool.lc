-------------------------------------------------------------------------------
-- DO NOT MODIFY THIS SEGMENT
-------------------------------------------------------------------------------

let TRUE  = \x y -> x
let FALSE = \x y -> y
let ITE   = \b x y -> b x y
let NOT   = \b x y -> b y x
let AND   = \b1 b2 -> ITE b1 b2 FALSE
let OR    = \b1 b2 -> ITE b1 TRUE b2

-------------------------------------------------------------------------------
-- YOU SHOULD ONLY MODIFY THE TEXT BELOW, JUST THE PARTS MARKED AS COMMENTS
-------------------------------------------------------------------------------

eval not_true :
  NOT TRUE
=d> (\b -> b FALSE TRUE) TRUE        
=a> TRUE FALSE TRUE                  
=d> (\x y -> x) FALSE TRUE           
=a> (\y -> FALSE) TRUE               
=b> FALSE 
  

eval and_true_false :
  AND TRUE FALSE
  =d> (\p q -> p q FALSE) TRUE FALSE   
  =a> TRUE FALSE FALSE                 
  =d> (\x y -> x) FALSE FALSE          
  =a> (\y -> FALSE) FALSE              
  =b> FALSE      

eval or_false_true :
  OR FALSE TRUE
  =d> (\p q -> p TRUE q) FALSE TRUE    
  =a> FALSE TRUE TRUE                  
  =d> (\x y -> y) TRUE TRUE            
  =a> (\y -> y) TRUE                   
  =b> TRUE
