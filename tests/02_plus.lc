-------------------------------------------------------------------------------
-- DO NOT MODIFY THIS SEGMENT
-------------------------------------------------------------------------------

let ZERO  = \f x -> x
let ONE   = \f x -> f x
let TWO   = \f x -> f (f x)
let THREE = \f x -> f (f (f x))
let FOUR  = \f x -> f (f (f (f x)))

let INC   = \n f x -> f (n f x)
let ADD   = \n m -> n INC m
let MUL   = \n m -> n (ADD m) ZERO

-------------------------------------------------------------------------------
-- YOU SHOULD ONLY MODIFY THE TEXT BELOW, JUST THE PARTS MARKED AS COMMENTS
-------------------------------------------------------------------------------

eval inc_one :
  INC ONE
  =d> (\n f x -> f (n f x)) ONE
  =a> \f x -> f (ONE f x)
  =d> \f x -> f ((\f x -> f x) f x)
  =a> \f x -> f (f x)
  =b> TWO

eval add_zero_zero :
  ADD ZERO ZERO
  =d> (\n m -> n INC m) ZERO ZERO
  =a> ZERO INC ZERO
  =d> (\f x -> x) INC ZERO
  =a> \x -> x                         
  =a> ZERO
  =b> ZERO

eval add_two_two :
  ADD TWO TWO
  =d> (\n m -> n INC m) TWO TWO
  =a> TWO INC TWO
  =d> (\f x -> f (f x)) INC TWO
  =a> INC (INC TWO)
  =d> (\n f x -> f (n f x)) (INC TWO)
  =a> \f x -> f ((INC TWO) f x)
  =d> \f x -> f ((\n f x -> f (n f x)) TWO f x)
  =a> \f x -> f (f (TWO f x))
  =d> \f x -> f (f ((\f x -> f (f x)) f x))
  =a> \f x -> f (f (f (f x)))
  =b> FOUR
