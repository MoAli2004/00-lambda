-------------------------------------------------------------------------------
-- DO NOT MODIFY THIS SEGMENT
-------------------------------------------------------------------------------

let ZERO  = \f x -> x
let ONE   = \f x -> f x
let TWO   = \f x -> f (f x)
let THREE = \f x -> f (f (f x))
let FOUR  = \f x -> f (f (f (f x)))

let INC   = \n f x -> f (n f x)
let ADD   = \n m -> n INC m
let MUL   = \n m -> n (ADD m) ZERO

-------------------------------------------------------------------------------
-- YOU SHOULD ONLY MODIFY THE TEXT BELOW, JUST THE PARTS MARKED AS COMMENTS
-------------------------------------------------------------------------------

eval inc_one :
  INC ONE
  =d> (\n f x -> f (n f x)) ONE        
  =a> (\f x -> f (ONE f x))            
  =d> (\f x -> f ((\f x -> f x) f x))  
  =b> (\f x -> f (f x))                


 

eval add_zero_zero :
  ADD ZERO ZERO
  =d> (\m n f x -> m f (n f x)) ZERO ZERO    
  =a> (\n f x -> ZERO f (n f x)) ZERO        
  =a> (\f x -> ZERO f (ZERO f x))            
  =d> (\f x -> (\f x -> x) f ((\f x -> x) f x)) 
  =b> (\f x -> x)

eval add_two_two :
  ADD TWO TWO
  =d> (\m n f x -> m f (n f x)) TWO TWO     
  =a> (\n f x -> TWO f (n f x)) TWO         
  =a> (\f x -> TWO f (TWO f x))             
  =d> (\f x -> (\f x -> f (f x)) f ((\f x -> f (f x)) f x))  
  =b> (\f x -> f (f (f (f x)))) 
